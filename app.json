{
  "expo": {
    "name": "Boxful App",
    "slug": "boxful-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "boxfulapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "light": "./assets/icons/ios-icon.png",
        "dark": "./assets/icons/ios-icon.png",
        "tinted": "./assets/icons/ios-icon.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFF4F1",
        "foregroundImage": "./assets/icons/android-icon.png",
        "backgroundImage": "./assets/icons/android-icon.png",
        "monochromeImage": "./assets/icons/android-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/android-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFF4F1",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "backgroundColor": "#FF4300"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
